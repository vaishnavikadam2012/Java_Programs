import java.io.*;
import java.util.*;
public class Solution{
      
        int fact=1;
        public  void factorial(int n){
        for(int i=1;i<=n; i++)
        {
         fact = fact*i;
        }
        System.out.println(fact);
        }
        
    public static void main(String[] args){
       Solution s=new Solution();
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       s.factorial(n);
          }
}
